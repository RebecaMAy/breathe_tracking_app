{\bkmkstart AAAAAAACSX}{\bkmkend AAAAAAACSX}
00419         {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAACHC" }{}}{\fldrslt {\cs37\ul\cf2 lastUpdatedOzono}}}
 = o3_ppm;\par
{\bkmkstart AAAAAAACSY}{\bkmkend AAAAAAACSY}
00420         {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAACHF" }{}}{\fldrslt {\cs37\ul\cf2 lastUpdatedCo2}}}
 = co2_ppm;\par
{\bkmkstart AAAAAAACSZ}{\bkmkend AAAAAAACSZ}
00421 \par
{\bkmkstart AAAAAAACTA}{\bkmkend AAAAAAACTA}
00422         {\cf20 // Comprobamos las alertas}\par
{\bkmkstart AAAAAAACTB}{\bkmkend AAAAAAACTB}
00423         {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAACTC" }{}}{\fldrslt {\cs37\ul\cf2 checkAlerts}}}
(co2_ppm, o3_ppm, temperatura_c, bat_porc);\par
{\bkmkstart AAAAAAACTD}{\bkmkend AAAAAAACTD}
00424 \par
{\bkmkstart AAAAAAACTE}{\bkmkend AAAAAAACTE}
00425         {\cf20 // Actualizamos la UI con los nuevos datos}\par
{\bkmkstart AAAAAAACTF}{\bkmkend AAAAAAACTF}
00426         {\cf20 // Obtenemos la hora actual para saber la ultima actualizacion de datos}\par
{\bkmkstart AAAAAAACTG}{\bkmkend AAAAAAACTG}
00427         String receptionTime = {\cf17 new} SimpleDateFormat({\cf22 "HH:mm"}, Locale.getDefault()).format({\cf17 new} Date());\par
{\bkmkstart AAAAAAACTH}{\bkmkend AAAAAAACTH}
00428         {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAACGI" }{}}{\fldrslt {\cs37\ul\cf2 dataHolder}}}
.timeData.postValue({\cf22 "\'DAltima conex. "} + receptionTime);\par
{\bkmkstart AAAAAAACTI}{\bkmkend AAAAAAACTI}
00429         {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAACGI" }{}}{\fldrslt {\cs37\ul\cf2 dataHolder}}}
.ozonoData.postValue(o3_ppm);\par
{\bkmkstart AAAAAAACTJ}{\bkmkend AAAAAAACTJ}
00430         {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAACGI" }{}}{\fldrslt {\cs37\ul\cf2 dataHolder}}}
.temperaturaData.postValue(temperatura_c);\par
{\bkmkstart AAAAAAACTK}{\bkmkend AAAAAAACTK}
00431         {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAACGI" }{}}{\fldrslt {\cs37\ul\cf2 dataHolder}}}
.co2Data.postValue(co2_ppm);\par
{\bkmkstart AAAAAAACTL}{\bkmkend AAAAAAACTL}
