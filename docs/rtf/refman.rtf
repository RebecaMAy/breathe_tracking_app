00403 \par
{\bkmkstart AAAAAAACSI}{\bkmkend AAAAAAACSI}
00404         {\cf20 // Parseamos los datos del beacon}\par
{\bkmkstart AAAAAAACSJ}{\bkmkend AAAAAAACSJ}
00405         {\cf18 float} o3_ppm = ((payload[2] & 0xFF) << 8 | (payload[1] & 0xFF)) / 1000.0f;\par
{\bkmkstart AAAAAAACSK}{\bkmkend AAAAAAACSK}
